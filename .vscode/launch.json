    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter (debug) HabitTracker",
            "request": "launch",
            "type": "dart",                       
            "program": "${workspaceFolder:HabitTracker}/lib/main.dart",
            "flutterMode": "debug",
            "deviceId": "chrome", 
            "args": ["--web-port", "8080"]
        },
        {
            "name": "HabitTracker (profile mode)",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder:HabitTracker}/lib/main.dart",
            "flutterMode": "profile",
            "deviceId": "chrome", 
            "args": ["--web-port", "8081"]
        },
        {
            "name": "HabitTracker (release mode)",
            "request": "launch",
            "type": "dart",
            "program": "${workspaceFolder:HabitTracker}/lib/main.dart",
            "flutterMode": "release",
            "deviceId": "chrome",
            "args": ["--web-port", "8082"]
        }

    ]
}


     
    